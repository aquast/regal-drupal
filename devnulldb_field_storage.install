<?php
/**
 * @file
 * Install, update and uninstall functions for the devnulldb field storage module.
 */

/**
 * Implements hook_install().
 */
function devnulldb_field_storage_install() {
  variable_set('field_storage_default', 'devnulldb_field_storage');
}

function devnulldb_field_storage_disable() {
  variable_del('field_storage_default');
}
